{
  "name": "react-big-calendar",
  "version": "0.12.3",
  "description": "Calendar! with events",
  "author": "Jason Quense <monastic.panic@gmail.com>",
  "repository": "intljusticemission/react-big-calendar",
  "license": "MIT",
  "main": "lib/index.js",
  "style": "lib/css/react-big-calendar.css",
  "files": [
    "lib/",
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src/",
    ".babelrc"
  ],
  "keywords": [
    "scheduler",
    "react-component",
    "react",
    "calendar",
    "events",
    "full calendar"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "clean:examples": "rimraf examples/static",
    "less": "lessc --autoprefix=\"ie >= 10, last 2 versions\" src/less/styles.less ./lib/css/react-big-calendar.css",
    "assets": "cpy src/less/* lib/less",
    "build": "npm run clean && babel src --out-dir lib && npm run assets && npm run less",
    "build:examples": "npm run clean:examples && webpack --config webpack/examples.config.js",
    "build:visual-test": "webpack --config webpack/visual-test.js",
    "examples": "npm run clean:examples && webpack-dev-server --inline --hot --config webpack/examples.config.js",
    "lint": "eslint src --ext .jsx --ext .js",
    "storybook": "start-storybook -p 9002",
    "test": "npm run lint",
    "tdd": "karma start",
    "release": "release",
    "postinstall": "postinstall-build lib \"npm run build\""
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "devDependencies": {
    "@kadira/storybook": "^1.28.1",
    "babel-cli": "^6.7.5",
    "babel-core": "^6.7.6",
    "babel-eslint": "^6.0.2",
    "babel-loader": "^6.2.4",
    "babel-preset-jason": "^2.0.0",
    "component-metadata-loader": "^2.1.0",
    "cpy": "^3.4.1",
    "css-loader": "^0.15.6",
    "eslint": "^3.0.0",
    "eslint-config-jason": "^4.0.0",
    "eslint-import-resolver-webpack": "^0.8.0",
    "eslint-plugin-import": "^2.0.0",
    "eslint-plugin-react": "^6.8.0",
    "file-loader": "^0.8.4",
    "globalize": "^0.1.1",
    "html-webpack-plugin": "^1.7.0",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "less-plugin-autoprefix": "^1.5.1",
    "lolex": "^1.4.0",
    "markdown-jsx-loader": "^2.0.1",
    "marked": "^0.3.5",
    "moment": "^2.17.1",
    "mt-changelog": "^0.6.1",
    "postcss-loader": "^0.6.0",
    "react": "^15.0.0",
    "react-bootstrap": "^0.28.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.0.0",
    "release-script": "^1.0.0",
    "rimraf": "^2.4.2",
    "style-loader": "^0.12.3",
    "url-loader": "^0.5.6",
    "webpack": "^1.9.11",
    "webpack-dev-server": "^1.9.0"
  },
  "dependencies": {
    "classnames": "^2.1.3",
    "date-arithmetic": "^3.0.0",
    "dom-helpers": "^2.3.0 || ^3.0.0",
    "invariant": "^2.1.0",
    "lodash": "^4.17.4",
    "postinstall-build": "^2.0.0",
    "react-overlays": "^0.6.0",
    "react-prop-types": "^0.4.0",
    "uncontrollable": "^3.3.1 || ^4.0.0",
    "warning": "^2.0.0"
  }
}
